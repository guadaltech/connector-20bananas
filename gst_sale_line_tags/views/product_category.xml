<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sequence_product_category_list_view_inherit" model="ir.ui.view">
        <field name="name">product.category.list.gst.sdc.product.inherit</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_list_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='display_name']" position="before">
                <field name="sequence" widget="handle" />
            </xpath>
        </field>
    </record>
    <record id="sequence_product_category_form_view_inherit" model="ir.ui.view">
        <field name="name">product.category.sequence</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='parent_id']" position="after">
                <field name="sequence" />
            </xpath>
        </field>
    </record>
</odoo>
