<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="sequence_bananas_client" model="ir.sequence">
            <field name="name">Clientes de 20 bananas</field>
            <field name="code">bananas.client</field>
            <field name="prefix">BNA/CL</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_bananas_product" model="ir.sequence">
            <field name="name">Productos de 20 bananas</field>
            <field name="code">bananas.product</field>
            <field name="prefix">BNA/PR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_bananas_product_pricelist" model="ir.sequence">
            <field name="name">Productos tarifas de 20 bananas</field>
            <field name="code">bananas.product.pricelist</field>
            <field name="prefix">BNA/TF</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
